<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="entidades.Produccion" table="ordenproduccion">
      <id  name="codigoOrdenProduccion" type="int">
            <column name="codigoOrdenProduccion"/>
      </id>
        <property name="fechaOrdenProduccion" type="date">
            <column name="fechaOrdenProduccion"/>
        </property>
        <property name="estadoOrdenProduccion" type="string">
            <column name="estadoOrdenProduccion"/>
        </property>
        <property name="cantidadProducir" type="int">
            <column name="cantidadProducir"/>
        </property>
      
            <many-to-one name="productoTerminado" column="codigoProductoTerminado" class="entidades.ProductoTerminado" not-null="true"/>
 
  </class>
</hibernate-mapping>
