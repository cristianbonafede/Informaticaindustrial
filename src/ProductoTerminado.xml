<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="entidades.ProductoTerminado" table="productoterminado">
       <id name="codigoProductoTerminado" type="int">
            <column name="codigoProductoTerminado"/>
        </id>
        
        <property name="descripcionProducto" type="string">
            <column name="descripcionProducto"/>
        </property>
        <property name="nombreProductoTerminado" type="string">
            <column name="nombreProductoTerminado"/>
        </property>
                <property name="cantidadDisponible" type="int">
            <column name="cantidadDisponible"/>
        </property>
        <property name="precioPorUnidad" type="int">
            <column name="precioPorUnidad"/>
        </property>
         
               
     </class>      
  
</hibernate-mapping>
